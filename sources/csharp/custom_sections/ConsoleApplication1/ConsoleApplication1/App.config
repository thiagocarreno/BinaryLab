<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="reportsSection" type="ConsoleApplication1.ReportsSection, ConsoleApplication1"/>
  </configSections>
  <reportsSection xmlns="ConsoleApplication1">
    <!--
		This is just a minimal sample configuration file that shows how to declare
		the configuration sections.

		Because an XML Schema Definition (XSD) is generated for each configuration
		section, it should be trivial to edit these files because you have
		IntelliSense on the XML definition.
		-->
    <reports>
      <report name="report1">
        <sheets>
          <sheet name="sheet1" />
          <sheet name="sheet2" />
        </sheets>
      </report>
      <report name="report2">
        <sheets>
          <sheet name="sheet1" />
          <sheet name="sheet2" />
        </sheets>
      </report>
    </reports>
  </reportsSection>
  
</configuration>